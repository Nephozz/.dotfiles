* {
    background: #1e1e2e;
    foreground: #cdd6f4;
    border: 2px solid #f38ba8;
    border-radius: 10px;
    padding: 10px;
    font: "JetBrains Mono 13";
}

window {
    background: #1e1e2e;
    border: 2px solid #f38ba8;
    border-radius: 12px;
}

element {
    padding: 5px 10px;
    border-radius: 8px;
}

element selected {
    background: #f38ba8;
    foreground: #1e1e2e;
}
